@model Bdl.BoletoBdl.Application.ViewModels.ContatoViewModel

<div class="form-group">
    @Html.LabelFor(model => model.ContatoTipoId, htmlAttributes: new { @class = "control-label col-md-2" })
    <div class="col-md-10">
        @Html.DropDownList("ContatoTipoId", (IEnumerable<SelectListItem>)ViewBag.ContatoTipoId, new { @class = "form-control" })
        @Html.ValidationMessageFor(model => model.ContatoTipoId, "", new { @class = "text-danger" })
    </div>
</div>

